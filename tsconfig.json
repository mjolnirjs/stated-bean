{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "incremental": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "stated-bean": ["src"]
    },
    "inlineSourceMap": true,
    "strict": true,
    "stripInternal": true,
    "target": "es5"
  }
}
