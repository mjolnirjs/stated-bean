{
  "name": "stated-bean",
  "version": "0.2.0-alpha.1",
  "repository": "git@github.com:mjolnirjs/stated-bean.git",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "es2015": "lib/es/index.js",
  "module": "lib/esm.js",
  "browser": "lib/umd.js",
  "unpkg": "lib/umd.min.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "parcel ./example/index.html --https",
    "start": "yarn dev",
    "serve": "serve dist",
    "build:cjs": "tsc -p src/tsconfig.cjs.json",
    "build:es": "tsc -p src/tsconfig.es.json",
    "build:bundle": "rollup -c node_modules/@1stg/rollup-config/module.js",
    "build:bundle:min": "cross-env NODE_ENV=production yarn build:bundle",
    "build:example": "parcel build ./example/index.html",
    "build": "rimraf dist lib && run-p build:**",
    "lint": "cross-env EFF_NO_LINK_RULES=true eslint . --ext js,md,mdx,ts,tsx -f friendly",
    "test": "jest --env=jsdom --coverage --preset ts-jest",
    "type-coverage": "type-coverage --ignore-catch --ignore-files 'lib/**/*' --strict"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@1stg/eslint-config": "^0.6.0",
    "@1stg/husky-config": "^0.3.0",
    "@1stg/lint-staged": "^0.5.0",
    "@1stg/prettier-config": "^0.2.0",
    "@1stg/rollup-config": "^0.2.2",
    "@1stg/tsconfig": "^0.3.0",
    "@abraham/reflection": "^0.5.1",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^16.9.0",
    "cross-env": "^5.2.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.3.0",
    "eslint-formatter-friendly": "^7.0.0",
    "husky": "^3.0.4",
    "injection-js": "^2.2.2",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.3",
    "serve": "^11.1.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "type-coverage": "^2.2.0",
    "typescript": "^3.6.2"
  },
  "alias": {
    "stated-bean": "./src"
  },
  "sideEffects": false,
  "typeCoverage": {
    "atLeast": 98
  }
}
